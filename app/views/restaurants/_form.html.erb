<%= simple_form_for(restaurant) do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :phone_number%>
  <%= f.input :category, :collection=> %w[Chinese Italian Japanese French Belgian], prompt: "Choose a category", label: "Cuisine Type" %>
  <%= f.submit class: "btn btn-primary hoverable" %>
<% end %>
